<p id="notice"><%= notice %></p>

<h1>Contents</h1>

<table>
  <thead>
    <tr>
      <th>Link</th>
      <th>UVA-only</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contents.each do |content| %>
      <tr>
        <td><%= link_to(content[:"path"], content[:"path"]) %></td>
        <td><%= content[:"uva-only"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<b><%= link_to 'New Content', new_content_path %> </b>


